{
      "name": "@nodeholder/pdata",
      "version": "0.1.7",
      "type": "module",
      "description": "PData service for secure file management",
      "keywords": [
            "pdata",
            "file",
            "auth",
            "api"
      ],
      "homepage": "https://github.com/study-groups/devops#readme",
      "bugs": {
            "url": "https://github.com/study-groups/devops/issues"
      },
      "repository": {
            "type": "git",
            "url": "git+https://github.com/study-groups/devops.git"
      },
      "license": "ISC",
      "author": "Mike Ricos",
      "exports": {
            ".": "./index.js"
      },
      "main": "./index.js",
      "bin": {
            "pdata-usermgr": "./manageUsers.js"
      },
      "files": [
            "index.js",
            "PData.js",
            "routes.js",
            "userUtils.js",
            "manageUsers.js",
            "auth.js",
            "dbUtils.js",
            "README.md",
            "tests/"
      ],

      "dependencies": {
            "express-session": "^1.17.0",
            "fs-extra": "^11.3.0",
            "multer": "^1.4.5-lts.1",
            "passport": "^0.6.0"
      },
      "peerDependencies": {
            "express": "^4.17.1 || ^5.0.0"
      },
      "devDependencies": {
            "express": "^5.0.0",
            "jest": "^29.7.0",
            "passport-local": "^1.0.0",
            "supertest": "^6.0.0"
      },
      "engines": {
            "node": ">=18.0.0"
      },
      "publishConfig": {
            "access": "public"
      },
      "scripts": {
            "setup-test-users": "node tests/setupTestUsers.js",
            "test": "npm run setup-test-users && NODE_ENV=test NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.js",
            "test:watch": "NODE_ENV=test NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.js --watch",
            "test:api": "NODE_ENV=test NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.js tests/api.test.js",
            "test:coverage": "NODE_ENV=test NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.js --coverage",
            "lint": "eslint .",
            "docs": "jsdoc -c jsdoc.json"
      }
}
